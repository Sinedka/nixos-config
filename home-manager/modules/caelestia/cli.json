{
  "theme": {
    "enableTerm": true,
    "enableHypr": true,
    "enableDiscord": true,
    "enableSpicetify": true,
    "enableFuzzel": true,
    "enableBtop": true,
    "enableGtk": true,
    "enableQt": true
  },
  "toggles": {
    "communication": {
      "discord": {
        "enable": true,
        "match": [
          {
            "class": "discord"
          }
        ],
        "command": [
          "vesktop"
        ],
        "move": true
      },
    },
    "music": {
      "spotify": {
        "enable": true,
        "match": [
          {
            "class": "Spotify"
          },
          {
            "initialTitle": "Spotify"
          },
          {
            "initialTitle": "Spotify Free"
          }
        ],
        "command": [
          "spicetify",
          "watch",
          "-s"
        ],
        "move": true
      },
      "feishin": {
        "enable": true,
        "match": [
          {
            "class": "feishin"
          }
        ],
        "move": true
      }
    },
    "sysmon": {
      "btop": {
        "enable": true,
        "match": [
          {
            "class": "btop",
            "title": "btop",
            "workspace": {
              "name": "special:sysmon"
            }
          }
        ],
        "command": [
          "btop",
        ]
      }
    },
    "todo": {
      "todoist": {
        "enable": true,
        "match": [
          {
            "class": "Todoist"
          }
        ],
        "command": [
          "todoist"
        ],
        "move": true
      }
    }
  }
}
